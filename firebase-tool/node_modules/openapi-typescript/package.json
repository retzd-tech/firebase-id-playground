{
  "name": "openapi-typescript",
  "description": "Generate TypeScript types from Swagger OpenAPI specs",
  "version": "4.5.0",
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 7.0.0"
  },
  "author": "drew@pow.rs",
  "license": "MIT",
  "bin": {
    "openapi-typescript": "bin/cli.js"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "browser": "./dist/esm/index.mjs",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./*": "./*"
  },
  "files": [
    "bin",
    "dist",
    "src",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/drwpow/openapi-typescript"
  },
  "keywords": [
    "swagger",
    "typescript",
    "ts",
    "dts",
    "openapi",
    "codegen",
    "generation",
    "openapi 3",
    "node"
  ],
  "bugs": {
    "url": "https://github.com/drwpow/openapi-typescript/issues"
  },
  "homepage": "https://github.com/drwpow/openapi-typescript#readme",
  "scripts": {
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json && renamer --find .js --replace .mjs \"dist/esm/**/*\"",
    "format": "npm run prettier -w .",
    "lint": "eslint .",
    "prepare": "npm run build",
    "pregenerate": "npm run build",
    "test": "mocha --recursive --parallel --timeout 30000",
    "test:coverage": "nyc --reporter=lcov mocha --recursive --parallel --timeout 30000 && codecov",
    "typecheck": "tsc --noEmit --project tsconfig.esm.json",
    "version": "npm run build"
  },
  "dependencies": {
    "hosted-git-info": "^3.0.8",
    "js-yaml": "^4.1.0",
    "kleur": "^4.1.4",
    "meow": "^9.0.0",
    "mime": "^3.0.0",
    "node-fetch": "^2.6.6",
    "prettier": "^2.5.1",
    "slash": "^3.0.0",
    "tiny-glob": "^0.2.9"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.1",
    "@types/mime": "^2.0.3",
    "@types/node-fetch": "^2.5.12",
    "@types/prettier": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "chai": "^4.3.4",
    "codecov": "^3.8.3",
    "eol": "^0.9.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "renamer": "^4.0.0",
    "typescript": "^4.5.4"
  }
}
